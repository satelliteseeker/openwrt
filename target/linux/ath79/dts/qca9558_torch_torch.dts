#include "qca955x.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "Torch Gigabit WiFi Router";
	compatible = "torch,torch", "qca,qca9558";

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};
};

&eth0 {
	status = "okay";

	phy-handle = <&phy0>;
	pll-data = <0x82000101 0x80000101 0x80001313>;
};

&eth1 {
	status = "okay";

	phy-handle = <&phy1>;
	pll-data = <0x03000101 0x80000101 0x80001313>;
};

&mdio0 {
	status = "okay";

	phy0: ethernet-phy@1 {
		reg = <1>;

		qca,ar8327-initvals = <
			0x04 0x87600000 /* PORT0 PAD MODE CTRL */
			0x7c 0x0000007e /* PORT0_STATUS */
			>;
	};

	phy1: ethernet-phy@2 {
		reg = <2>;

		qca,ar8327-initvals = <
			0x04 0x87600000 /* PORT1 PAD MODE CTRL */
			0x7c 0x0000007e /* PORT1_STATUS */
			>;
	};
};

&pcie0 {
	status = "okay";

	wifi@0,0 {
		compatible = "pci168c,003c";
		reg = <0x0000 0 0 0 0>;
	};
};

&spi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x040000 0x010000>;
				read-only;
			};

			partition@50000 {
				label = "art";
				reg = <0x50000 0x010000>;
				read-only;
			};

			partition@60000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x060000 0xe50000>;
			};

			partition@eb0000 {
				label = "manufacture";
				reg = <0xeb0000 0x100000>;
				read-only;
			};

			partition@fb0000 {
				label = "backup";
				reg = <0xfb0000 0x010000>;
				read-only;
			};

			partition@fc0000 {
				label = "storage";
				reg = <0xfc0000 0x040000>;
				read-only;
			};
		};
	};
};

&usb_phy1 {
	status = "okay";
};

&usb1 {
	status = "okay";
};

&wmac {
	status = "okay";

	qca,no-eeprom;
};
